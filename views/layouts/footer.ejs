<script>
    $.ajax({
        url: '/footer-news', success: function (result) {
            var data = (result);
            for (var i = 0; i < 3; i++) {
                var x1 = "#footerNewsImage" + i;
                var x2 = "#footerNewsLink" + i;
                var x3 = "#footerNewsTitle" + i;
                var x4 = "#footerNewsSource" + i;
                $(x1).attr("src", data[i].img_url);
                $(x2).attr("href", data[i].url);
                $(x3).text(data[i].title);
                $(x4).text(data[i].source);
            }

        }
    })

</script>
